    "@nestjs/config": "^4.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jszip": "^3.4.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
